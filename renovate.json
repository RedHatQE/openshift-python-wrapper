{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": [
    "main",
    "v4.15",
    "v4.14"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "xmltodict",
        "colorlog",
        "kubernetes",
        "python-benedict",
        "packaging",
        "python-simple-logger",
        "jinja2",
        "timeout-sampler",
        "ipdb",
        "pytest",
        "requests"
      ],
      "groupName": "poetry-deps"
    }
  ]
}
