{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["main", "v4.16", "v4.15", "v4.14"],
  "ignoreDeps": ["kubernetes", "openshift"],
  "packageRules": [
    {
      "baseBranchList": ["v4.16", "v4.15", "v4.14"],
      "enabled": false
    },
    {
      "matchPackagePatterns": ["*"],
      "groupName": "poetry-deps"
    }
  ]
}
